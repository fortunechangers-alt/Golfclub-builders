# Golf Club Builders - Git Ignore File
# This prevents sensitive config files from being overwritten

# Dependencies
vendor/
node_modules/

# Environment files
.env
.env.local
.env.production

# Configuration files with sensitive data
app/Config/SendGrid.php
app/Config/Database.php
app/Config/App.php

# Server-specific files
.htaccess
writable/logs/*
writable/session/*
writable/cache/*

# Backup files
*.backup
*.bak

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Temporary files
*.tmp
*.temp